{
  "name": "hi5-redemption",
  "version": "1.0.0",
  "description": "Display local HI5 redemption centers on the island.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production npm run clean && npm run build:compile && npm run build:cname",
    "build:compile": "NODE_ENV=production webpack --progress --hide-modules --config config/webpack.production.js",
    "build:cname": "echo www.redeemhi5.com > dist/CNAME",
    "start": "NODE_ENV=development webpack-dev-server --inline --hot --config config/webpack.development.js",
    "start:prod": "NODE_ENV=production npm run build && hs dist",
    "test": "NODE_ENV=test karma start karma.conf.js",
    "test:watch": "karma start karma.conf.js --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keokilee/hi5-redemption.git"
  },
  "author": "George Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keokilee/hi5-redemption/issues"
  },
  "homepage": "https://github.com/keokilee/hi5-redemption#readme",
  "dependencies": {
    "google-maps": "^3.2.1",
    "http-server": "^0.9.0",
    "moment": "^2.12.0",
    "node-sass": "^3.9.3",
    "scss": "^0.2.4",
    "vue": "^1.0.18",
    "vue-infinite-loading": "^1.1.0",
    "vue-router": "^0.7.8",
    "vuex": "^0.6.2"
  },
  "devDependencies": {
    "appcache-webpack-plugin": "^1.2.1",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.15.2",
